{
	"AWSEBDockerrunVersion": 2,
	"containerDefenisions": [{
			"name": "client",
			"image": "dilshadcp/multi-client",
			"hostname": "client",
			"essensial": false,
			"memory": 128
		},
		{
			"name": "server",
			"image": "dilshadcp/multi-server",
			"hostname": "api",
			"essensial": false,
			"memory": 128
		},
		{
			"name": "worker",
			"image": "dilshadcp/multi-worker",
			"hostname": "worker",
			"essensial": false,
			"memory": 128
		},
		{
			"name": "nginx",
			"image": "dilshadcp/multi-nginx",
			"hostname": "nginx",
			"essensial": true,
			"portMappings": [{
				"hostsPort": 80,
				"containerPort": 80
			}],
			"links": ["client", "server"],
			"memory": 128
		}
	]
}